@model PagedList.IPagedList<ReadBookmarkWeb.Models.Link>
@using PagedList.Mvc;

@{
    ViewBag.Title = "Home Page";
}

<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-lg-5">
                <a href="@item.Href" target="_blank">@Html.Raw(@item.Title)</a>
            </div>

        }

    </div>

    @Html.PagedListPager(Model, page => Url.Action("Index", new { page, isRandom = false }))
</div>



